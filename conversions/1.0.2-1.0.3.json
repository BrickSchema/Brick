    "brick": "https://brickschema.org/schema/1.0.2/Brick#"
  },
  "operations": [
    {
      "description": "Add hasLocation",
      "query": "INSERT{ ?subject brick:hasLocation ?object . } WHERE { ?subject brickframe:isLocatedIn ?object . }"
