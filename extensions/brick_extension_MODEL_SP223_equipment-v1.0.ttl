  sh:property [
      sh:path s223:commandedByProperty ;
      rdfs:comment "An instance of Actuator must be associated with any number of actuatable property instances using the relation 'commanded by Property'." ;
      sh:class s223:ActuatableProperty ;
      sh:minCount 1 ;
    ] ;
.
s223:AirFilter
  a s223:Class ;
  a sh:NodeShape ;
  rdfs:comment "A device that removes particulates from gasses such as air." ;
  rdfs:label "Air filter" ;
