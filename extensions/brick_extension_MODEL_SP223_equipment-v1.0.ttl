s223:DifferentialSensor
  a s223:Class ;
  a sh:NodeShape ;
  rdfs:comment "A sensor that measures the difference of a quantity between any two points in the system." ;
  rdfs:label "Differential sensor" ;
  rdfs:subClassOf s223:AbstractSensor ;
  sh:property [
      sh:path (
          s223:observes
          qudt:hasQuantityKind
        ) ;
      rdfs:comment "A Differential Sensor must be defined in terms of the QuantityKind that is being measured" ;
      sh:class qudt:QuantityKind ;
      sh:minCount 1 ;
    ] ;
  sh:xone (
      [
        sh:property [
            sh:path s223:hasObservationLocationHigh ;
            rdfs:comment "A DifferentialSensor must be associated with exactly 1 of Connectable, Connection, Segment, or ConnectionPoint by hasObservationLocationHigh." ;
            sh:class s223:Connectable ;
          ] ;
      ]
      [
        sh:property [
            sh:path s223:hasObservationLocationHigh ;
            rdfs:comment "A DifferentialSensor must be associated with exactly 1 of Connectable, Connection, Segment, or ConnectionPoint by hasObservationLocationHigh." ;
            sh:class s223:Connection ;
          ] ;
      ]
      [
        sh:property [
            sh:path s223:hasObservationLocationHigh ;
            rdfs:comment "A DifferentialSensor must be associated with exactly 1 of Connectable, Connection, Segment, or ConnectionPoint by hasObservationLocationHigh." ;
            sh:class s223:Segment ;
          ] ;
      ]
      [
        sh:property [
            sh:path s223:hasObservationLocationHigh ;
            rdfs:comment "A DifferentialSensor must be associated with exactly 1 of Connectable, Connection, Segment, or ConnectionPoint by hasObservationLocationHigh." ;
            sh:class s223:ConnectionPoint ;
          ] ;
      ]
    ) ;
  sh:xone (
      [
        sh:property [
            sh:path s223:hasObservationLocationLow ;
            rdfs:comment "A DifferentialSensor must be associated with exactly 1 of Connectable, Connection, Segment, or ConnectionPoint by hasObservationLocationLow." ;
            sh:class s223:Connectable ;
          ] ;
      ]
      [
        sh:property [
            sh:path s223:hasObservationLocationLow ;
            rdfs:comment "A DifferentialSensor must be associated with exactly 1 of Connectable, Connection, Segment, or ConnectionPoint by hasObservationLocationLow." ;
            sh:class s223:Connection ;
          ] ;
      ]
      [
        sh:property [
            sh:path s223:hasObservationLocationLow ;
            rdfs:comment "A DifferentialSensor must be associated with exactly 1 of Connectable, Connection, Segment, or ConnectionPoint by hasObservationLocationLow." ;
            sh:class s223:Segment ;
          ] ;
      ]
      [
        sh:property [
            sh:path s223:hasObservationLocationLow ;
            rdfs:comment "A DifferentialSensor must be associated with exactly 1 of Connectable, Connection, Segment, or ConnectionPoint by hasObservationLocationLow." ;
            sh:class s223:ConnectionPoint ;
          ] ;
      ]
    ) ;
.
