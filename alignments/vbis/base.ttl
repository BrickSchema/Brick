@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix vbis: <https://brickschema.org/schema/Brick/alignments/vbis#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

vbis:VBISTag    a   owl:Class ;
    skos:definition "A VBIS Tag, as defined by https://vbis.com.au/" ;
.

vbis:hasTag    a   brick:EntityProperty ;
    rdfs:range vbis:VBISTagShape ;
.

vbis:VBISTagShape   a   sh:NodeShape ;
    sh:property [
        sh:path vbis:tag ;
        sh:datatype xsd:string ;
    ] ;
.
